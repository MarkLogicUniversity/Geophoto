{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./README.md", "./lib"],
        "exclude": [],
        "includePattern": ".+\\.(js|md)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
    ],
    "templates": {
        "default": {
            "outputSourceFiles": false
        },
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true,
        "lenient": false
    }
}
